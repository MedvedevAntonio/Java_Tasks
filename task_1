public class Umnojenie {
    public static void main(String[] args) {
        System.out.println ("-------------------");
        System.out.println ("Таблица умножения");
        for (int i=1; i<=10; i++) {
            System.out.println ("5*" + i + "=" + 5*i);
            }
        System.out.println ("-------------------");
    }
}
